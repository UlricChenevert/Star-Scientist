{

    "files.exclude": {
 
        "**/*.js": {"when": "$(basename).ts"},

        "**/*.css": {"when": "$(basename).less"},

        "**/*?.js": {"when": "$(basename).tsx"},

        "**/*.d.ts": {"when": "$(basename).ts"},

        "**/*.js.map": true,

        "*/obj/**":true,

        "*/bin/**":true,

        "*/node_modules/**":true,

    }

}